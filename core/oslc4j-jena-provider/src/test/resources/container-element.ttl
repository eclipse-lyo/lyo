@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix oslc:  <http://open-services.net/ns/core#> .

<urn:test:containerA>  a  <http://locahost:7001/vocabularyContainer> ;
        <http://locahost:7001/vocabulary/childrenA>
                [ a       rdf:Alt ;
                  rdf:_1  <urn:test:A> ;
                  rdf:_2  <urn:test:B>
                ] ;
        <http://locahost:7001/vocabulary/childrenB>
                [ a       rdf:Bag ;
                  rdf:_1  <urn:test:A> ;
                  rdf:_2  <urn:test:B>
                ] ;
        <http://locahost:7001/vocabulary/childrenL>
                ( <urn:test:A> <urn:test:B> ) ;
        <http://locahost:7001/vocabulary/childrenS>
                [ a       rdf:Seq ;
                  rdf:_1  <urn:test:A> ;
                  rdf:_2  <urn:test:B>
                ] .

<urn:test:A>  a      <http://locahost:7001/vocabularyElement> ;
        <http://locahost:7001/vocabulary/name>
                "A" .

<urn:test:B>  a      <http://locahost:7001/vocabularyElement> ;
        <http://locahost:7001/vocabulary/name>
                "B" .

